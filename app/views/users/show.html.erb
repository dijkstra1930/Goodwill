<p id="notice"><%= notice %></p>
<% provide(:title, @user.username) %>
<div class="container span12">
  <% if @user.products.any? %>
      <div class="heading center"><h3>Your Products: <%= @user.products.count %> items</h3></div>

        <%= render partial: 'shared/feed_item', collection: @products %>
        <%= render @products %>
      <%= will_paginate @products %>
  <% end %>
</div>
